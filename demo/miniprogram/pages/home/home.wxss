/* pages/home/home.wxss */

input {
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background: url('data:image/png;base64,') #f4f4f4;
  background-size: 40rpx;
  background-repeat: no-repeat;
  padding-left: 40rpx;
  background-position: left center;
  border-radius: 50px;
  color: black;
  font-size: 12px;
}

scroll-view {
  white-space: nowrap;
}

scroll-view view {
  display: inline-block;
  width: 100rpx;
}



::-webkit-scrollbar {
  height: 0px;
  opacity: 0;
  /*去除滚动条*/
}

.bar {
  margin-left: 50rpx;
  margin-right: 50rpx;
  text-align: center;
}

.bar view {
  padding-left: 15rpx;
  padding-right: 15rpx;
}

.current {
  color: pink;
  padding-bottom: 15rpx;
  border-bottom: 5rpx solid pink;
}

.videoModule {
  display: block;
  margin-top: 25rpx;
  width: 750rpx;
  height: 620rpx;
  border-bottom: solid #eaeaea;
}

.video image {
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 710rpx;
  height: 450rpx;
  border-radius: 10rpx;
}

.videoTitle image {
  margin-left: 20rpx;
  margin-top: 10rpx;
  width: 80rpx;
  height: 80rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
  float: left;
}

.videoTitle text {
  width: 370rpx;
  float: left;
  display: block;
  margin-top: 10rpx;
  margin-left: 10rpx;
  font-size: 14px;
}

.category {
  width: 120rpx;
  height: 60rpx;
  background-color: #eaeaea;
  float: left;
  margin-top: 45rpx;
  margin-left: 50rpx;
  border-radius: 20rpx;
}

.category text {
  width: 100rpx;
  height: 40rpx;
  font-size: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.share image {
  margin-left: 20rpx;
  margin-top: 50rpx;
  width: 50rpx;
  height: 50rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
  float: left;
}

.author {
  width: 100%;
  display: flex;
  margin-left: 110rpx;
  margin-top: 10rpx;
  white-space:pre-line;
}

.author text {
  font-size: 12px;
  color:#958888;
}
.author text:nth-of-type(2){
  margin-top: -10rpx;
  padding-left:20rpx;
  padding-right: 20rpx; 
}
